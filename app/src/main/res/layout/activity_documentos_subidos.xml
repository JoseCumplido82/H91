<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ActivityDocumentosSubidos">

    <TextView
        android:id="@+id/txt_documentos2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:gravity="center"
        android:text="@string/documentos_subidos2"
        android:textColor="@color/cardview_dark_background"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_documentos"
        android:layout_width="308dp"
        android:layout_height="190dp"
        android:layout_marginTop="40dp"
        android:clickable="true"
        android:focusable="true"
        android:scrollbars="vertical"
        app:layout_constraintEnd_toEndOf="@+id/txt_documentos2"
        app:layout_constraintHorizontal_bias="0.521"
        app:layout_constraintStart_toStartOf="@+id/txt_documentos2"
        app:layout_constraintTop_toBottomOf="@+id/txt_documentos2" />

    <Button
        android:id="@+id/bt_volver13"
        android:layout_width="160dp"
        android:layout_height="60dp"
        android:layout_marginTop="52dp"
        android:text="@string/volver"
        app:backgroundTint="@color/cardview_dark_background"
        app:layout_constraintEnd_toEndOf="@+id/rv_documentos"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="@+id/rv_documentos"
        app:layout_constraintTop_toBottomOf="@+id/rv_documentos" />


</androidx.constraintlayout.widget.ConstraintLayout>